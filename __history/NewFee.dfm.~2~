  object qryFeeBasisList: TOraQuery
    Session = dmSaveDoc.orsInsight
    SQL.Strings = (
      'select CODE, CODE ||'#39' - '#39'||DESCR as descr, BILLTYPE '
      'from scalecost'
      'order by descr')
    Left = 257
    Top = 288
  end
  object qryTaxType: TOraQuery
