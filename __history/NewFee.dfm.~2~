  object qFeeEarners: TOraQuery
    Session = dmSaveDoc.orsInsight
    SQL.Strings = (
      'SELECT DISTINCT CODE, NAME, DEPT'
      'FROM'
      '(SELECT '
      'CODE, '
      'NAME,'
      'DEPT '
      'FROM EMPLOYEE '
      'WHERE'
      'ACTIVE = '#39'Y'#39' AND '
      'ISFEEEARNER = '#39'Y'#39
      'UNION ALL'
      'SELECT'
      'CODE,'
      'NAME,'
      'DEPT'
      'FROM EMPLOYEE'
      'WHERE'
      'CODE = :CODE)'
      'ORDER BY CODE')
    AutoCommit = False
    Left = 304
    Top = 72
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'CODE'
        Value = nil
      end>
    object qFeeEarnersCODE: TStringField
      FieldName = 'CODE'
      Size = 3
    end
    object qFeeEarnersNAME: TStringField
      FieldName = 'NAME'
      Size = 40
    end
    object qFeeEarnersDEPT: TStringField
      FieldName = 'DEPT'
      Size = 3
    end
  end
  object qryScaleCost: TOraQuery
    Session = dmSaveDoc.orsInsight
    SQL.Strings = (
      
        'SELECT nvl(AMOUNT,0) as amount, nvl(RATE,0) as rate, DESCR, UNIT' +
        ', ZERO_FEE '
      'FROM SCALECOST '
      'WHERE CODE = :P_Code')
    Left = 344
    Top = 120
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'P_Code'
        Value = nil
      end>
  end
  object qryFee: TSmartQuery
    Session = dmSaveDoc.orsInsight
    SQL.Strings = (
      'SELECT F.*, F.ROWID '
